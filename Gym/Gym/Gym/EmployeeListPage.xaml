<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gym.EmployeeListPage">
    <ContentPage.Content>
        <ListView x:Name="EmployeeListView" HorizontalOptions="FillAndExpand"
                VerticalOptions="FillAndExpand"
                  ItemSelected="Employee_ItemSelected">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding EmpName}" Detail="{Binding EmpId}"/>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        
    </ContentPage.Content>
</ContentPage>